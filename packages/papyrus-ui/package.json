{
  "name": "papyrus-ui",
  "version": "0.7.0",
  "description": "The UI kit with fundamental React components based on the Papyrus UI design system.",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/es/plugin/index.js",
      "require": "./dist/cjs/plugin/index.js",
      "types": "./dist/types/plugin/index.d.ts"
    }
  },
  "author": "Sergey Murawsky <mrwskdv@gmail.com>",
  "keywords": [],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrwskdv/papyrus-ui.git"
  },
  "scripts": {
    "build": "vite build",
    "test": "jest --config jest.config.mjs --passWithNoTests",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "tailwindcss": "^3 || ^4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-transition-group": "^4.4.6",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-banner2": "^1.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "4.7.4",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.7",
    "@react-hook/debounce": "^4.0.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react-icons": "5.4.0",
    "react-number-format": "^5.3.1",
    "react-transition-group": "^4.4.5"
  }
}
